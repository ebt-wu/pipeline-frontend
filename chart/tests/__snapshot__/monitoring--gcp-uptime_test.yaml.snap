Default Uptime Check for Extension Manager UI Site matches the Snapshot:
  1: |
    apiVersion: monitoring.cnrm.cloud.google.com/v1beta1
    kind: MonitoringUptimeCheckConfig
    metadata:
      name: extension-manager-ui-dev
      namespace: root-universe
    spec:
      displayName: HTTP Uptime Config for [extension-manager-ui-dev]
      httpCheck:
        path: /en/assets/cdm.json
        requestMethod: GET
        useSsl: true
        validateSsl: true
      monitoredResource:
        filterLabels:
          host: test-domain
          project_id: sap-gcp-dxp-test
        type: uptime_url
      period: 60s
      projectRef:
        external: sap-gcp-dxp-test
      timeout: 30s
  2: |
    apiVersion: monitoring.cnrm.cloud.google.com/v1beta1
    kind: MonitoringUptimeCheckConfig
    metadata:
      name: extension-manager-ui-dev-wc
      namespace: root-universe
    spec:
      displayName: HTTP Uptime Config for [extension-manager-ui-dev-wc]
      httpCheck:
        path: /assets/cdm.json
        requestMethod: GET
        useSsl: true
        validateSsl: true
      monitoredResource:
        filterLabels:
          host: test-domain-wc
          project_id: sap-gcp-dxp-test
        type: uptime_url
      period: 60s
      projectRef:
        external: sap-gcp-dxp-test
      timeout: 30s

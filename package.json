{
  "name": "pipeline-ui",
  "version": "0.0.1",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "start": "ng serve",
    "build": "ng build --configuration production --localize --output-path=dist",
    "test": "yarn test:unit",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test:unit": "jest --maxWorkers=40%",
    "test:cov": "yarn test:unit --coverage",
    "extract": "ng xi18n --output-path=src/locale",
    "postinstall": "husky install"
  },
  "private": true,
  "prettier": "@dxp/prettier-config",
  "dependencies": {
    "@angular/animations": "15.2.8",
    "@angular/cdk": "15.2.8",
    "@angular/common": "15.2.8",
    "@angular/compiler": "15.2.8",
    "@angular/core": "15.2.8",
    "@angular/forms": "15.2.8",
    "@angular/localize": "15.2.8",
    "@angular/platform-browser": "15.2.8",
    "@angular/platform-browser-dynamic": "15.2.8",
    "@angular/router": "15.2.8",
    "@dxp/ngx-core": "0.15.6",
    "@dynatrace/dtrum-api-types": "1.257.2",
    "@fundamental-ngx/core": "0.40.3",
    "@fundamental-ngx/platform": "0.40.3",
    "@fundamental-styles/common-css": "0.28.3",
    "@luigi-project/client": "2.2.0",
    "@luigi-project/client-support-angular": "2.2.0",
    "@sap-theming/theming-base-content": "11.3.2",
    "fundamental-styles": "0.28.3",
    "rxjs": "7.8.1",
    "tslib": "2.5.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.8",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.2.8",
    "@angular/compiler-cli": "15.2.8",
    "@dxp/config-prettier": "1.0.2",
    "@dxp/eslint-config-angular": "1.0.2",
    "@luigi-project/testing-utilities": "2.2.0",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.39.0",
    "eslint-plugin-jest": "27.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-mock-extended": "3.0.4",
    "jest-preset-angular": "13.0.1",
    "lint-staged": "13.2.2",
    "ng-mocks": "14.10.0",
    "prettier": "2.8.8",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{ts,css,md,html,json,scss}": "prettier -w",
    "*.ts": "eslint --cache"
  },
  "packageManager": "yarn@3.0.0"
}

{
  "name": "pipeline-ui",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.16.0",
    "yarn": ">=3"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "start": "ng serve",
    "start-local": "ng serve --configuration=local",
    "build": "ng build --configuration production --localize --output-path=dist --stats-json ",
    "test": "yarn test:unit",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test:unit": "jest --maxWorkers=40%",
    "test:cov": "yarn test:unit --coverage",
    "format:fix": "prettier --write .",
    "extract": "ng xi18n --output-path=src/locale",
    "codegen": "graphql-codegen -r ts-node/register --config codegen.ts"
  },
  "dependencies": {
    "@angular/animations": "^16.2.2",
    "@angular/cdk": "^16.2.1",
    "@angular/common": "^16.2.2",
    "@angular/compiler": "^16.2.2",
    "@angular/core": "^16.2.2",
    "@angular/elements": "^16.2.2",
    "@angular/forms": "^16.2.2",
    "@angular/localize": "^16.2.2",
    "@angular/platform-browser": "^16.2.2",
    "@angular/platform-browser-dynamic": "^16.2.2",
    "@angular/router": "^16.2.2",
    "@apollo/client": "^3.8.1",
    "@dxp/ngx-core": "0.16.25",
    "@dynatrace/dtrum-api-types": "1.275.1",
    "@fundamental-ngx/core": "0.46.1",
    "@fundamental-ngx/platform": "0.46.1",
    "@fundamental-styles/common-css": "0.30.2",
    "@luigi-project/client": "2.7.1",
    "@luigi-project/client-support-angular": "2.7.1",
    "@sap-theming/theming-base-content": "11.8.0",
    "apollo-angular": "^5.0.0",
    "fundamental-styles": "0.28.3",
    "graphql": "^16.8.0",
    "graphql-sse": "^2.3.0",
    "open": "^9.1.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.2",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-angular": "4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@luigi-project/testing-utilities": "2.7.1",
    "@types/jest": "29.5.8",
    "@types/node": "18.18.9",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-plugin-jest": "27.6.0",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "jest-preset-angular": "13.1.4",
    "openid-client": "^5.5.0",
    "prettier": "3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "5.1.x"
  },
  "packageManager": "yarn@3.6.4"
}

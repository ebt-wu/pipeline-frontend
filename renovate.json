{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Automatically merge non-major updates",
      "matchUpdateTypes": ["minor", "digest", "patch"],
      "automerge": true
    },
    {
      "matchPackagePatterns": ["^@luigi-project/*"],
      "groupName": "luigi"
    }
  ],
  "hostRules": [
    {
      "matchHost": "europe-docker.pkg.dev",
      "authType": "Basic",
      "encrypted": {
        "token": ""
      }
    },
    {
      "matchHost": "eu.gcr.io",
      "authType": "Basic",
      "encrypted": {
        "token": ""
      }
    }
  ]
}
